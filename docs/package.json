{
  "name": "react-player-controls-docs",
  "scripts": {
    "pug": "pug index.pug",
    "pug:watch": "pug -w index.pug",
    "sass": "node-sass --recursive --output build --source-map true --source-map-contents src/sass",
    "sass:watch": "npm run sass -- --watch",
    "js": "rollup -c -- src/js/demos.js",
    "js:watch": "rollup -w -c -- src/js/demos.js",
    "serve": "live-server --port=8293",
    "start": "npm-run-all --parallel '*:watch' 'serve'"
  },
  "private": true,
  "dependencies": {
    "pug": "^2.0.0-beta5"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015-rollup": "^1.2.0",
    "live-server": "^1.1.0",
    "livereload": "^0.5.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-watch": "^2.5.0"
  }
}
